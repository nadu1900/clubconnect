import React from 'react';
import eventData from './Event.json';
import eventTData from './EventT.json';
import eventLaf from './LafMockData.json';
import eventMor from './MoravianMockData.json';
import EventDetail from './EventDetail';

const CombinedEvents = () => {
    // Combine the event data from two different sources
    const combinedEvents = [...eventData, ...eventTData, ...eventMor, ...eventLaf];

    // Optional: Sort the combined events by date if each event has a 'date' attribute
    // Assuming dates are in ISO format or similar that can be directly compared
    combinedEvents.sort((a, b) => new Date(a.date) - new Date(b.date));

    return (
        <EventDetail 
        eventsData={combinedEvents} />
    );
};

export default CombinedEvents
